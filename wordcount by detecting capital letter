import java.io.*;
import java.util.*;

class GFG {
	public static void main (String[] args)
	{
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		for(int j=0;j<a;j++)
		{
		    String b=sc.next();
		    String c[]=b.split("(?=[A-Z])");
		    int d=c.length;
		    System.out.println(d);
		}
	}
}

